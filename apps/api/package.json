  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --config tsconfig.build.json",
    "check": "tsc --noEmit --skipLibCheck",
    "format": "prettier --write \"{src,test}/**/*.(t|j)s*\"",
    "start": "node -r ts-node/register -r tsconfig-paths/register dist/main.js",
    "start:queue:dev": "dotenv -e .env.development -- ts-node-dev src/worker.ts",
    "start:dev": "dotenv -e .env.development -- ts-node-dev src/main.ts",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "test": "dotenv -e .env.test -- jest -i --detectOpenHandles",
    "deploy:build": "pnpm --prefix ../../libs/prisma gen && pnpm build"
  },
  "files": ["dist", "templates", "schema.graphql"],
