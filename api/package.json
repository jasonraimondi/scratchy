{
  "name": "@package/server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "PORT=4000 tsx watch src/main.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "gen": "prisma generate",
    "start": "node --enable-source-maps dist/main.js",
    "db:migrate": "dotenv -f .env.development -- pnpm prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:migrate:test": "dotenv -f .env.test -- pnpm prisma migrate deploy",
    "db:seed": "NODE_ENV=development tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/csrf-protection": "^6.0.0",
    "@fastify/helmet": "^10.0.2",
    "@fastify/oauth2": "^6.1.0",
    "@fastify/rate-limit": "^7.5.0",
    "@fastify/websocket": "^7.1.1",
    "@jmondi/route-strings": "^1.3.2",
    "@prisma/client": "^4.6.1",
    "@trpc/client": "10.0.0-rc.7",
    "@trpc/server": "10.0.0-rc.7",
    "abort-controller": "^3.0.0",
    "axios": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "bullmq": "^3.2.0",
    "dotenv-flow": "^3.2.0",
    "fastify": "^4.9.2",
    "html-to-text": "^8.2.1",
    "immer": "^9.0.16",
    "jsonwebtoken": "^8.5.1",
    "mjml": "^4.13.0",
    "ms": "^2.1.3",
    "nodemailer": "^6.8.0",
    "nunjucks": "^3.2.3",
    "octokit": "^2.0.10",
    "source-map-support": "^0.5.21",
    "superjson": "^1.11.0",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "ws": "^8.11.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@jmondi/tsconfig": "^0.1.0",
    "@ngneat/falso": "^6.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/html-to-text": "^8.1.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mjml": "^4.7.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemailer": "^6.4.6",
    "@types/nunjucks": "^3.2.1",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.6.1",
    "prisma-zod-generator": "^0.7.0",
    "start-server-and-test": "^1.14.0",
    "tsx": "^3.12.1",
    "typescript": "^4.8.4",
    "wait-port": "^1.0.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
