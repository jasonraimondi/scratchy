{
  "name": "@lib/prisma",
  "version": "1.0.0",
  "files": [
    "dist",
    "prisma"
  ],
  "main": "src/index.ts",
  "publishConfig": {
    "main": "dist/index.js"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "db:seed": "ts-node prisma/seed.ts",
    "db:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "gen": "prisma generate",
    "gen:watch": "prisma generate --watch",
    "format": "prisma format && prettier --write \"prisma/**/*.(t|j)s*\""
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.9",
    "@nestjs/core": "^9.0.9",
    "@nestjs/graphql": "^10.0.21",
    "@ngneat/falso": "^6.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^18.7.3",
    "@types/uuid": "^8.3.4",
    "bcryptjs": "^2.4.3",
    "dotenv-cli": "^6.0.0",
    "graphql": "^16.5.0",
    "prisma": "^4.2.1",
    "prisma-nestjs-graphql": "^17.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@jmondi/prisma-generator-nestjs-graphql": "^1.0.0",
    "@prisma/client": "^4.2.1",
    "class-validator": "0.13.2",
    "cuid": "^2.1.8",
    "prisma-joi-generator": "^0.2.0"
  }
}
